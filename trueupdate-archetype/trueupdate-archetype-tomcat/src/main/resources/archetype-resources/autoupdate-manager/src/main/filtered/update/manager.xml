#set( $symbol_pound = '#' )
#set( $symbol_dollar = '$' )
#set( $symbol_escape = '\' )
<?xml version="1.0" encoding="UTF-8"?>
<manager xmlns="https://trueupdate.java.net/xml/ns/config"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="https://trueupdate.java.net/xml/ns/config https://trueupdate.java.net/xml/ns/config-1.0.xsd"
         version="1.0">
    <updateService>
        <uri>${symbol_dollar}{manager.updateService.uri}</uri>
    </updateService>
    <updateTimer>
        <period>${symbol_dollar}{manager.updateTimer.period}</period>
        <unit>minutes</unit>
    </updateTimer>
    <messaging>
        <connectionFactory>${parentArtifactId}/connection-factory</connectionFactory>
        <from>${parentArtifactId}/manager</from>
    </messaging>
</manager>
